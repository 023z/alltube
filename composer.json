{
    "name": "rudloff/alltube",
    "description": "HTML GUI for youtube-dl",
    "license": "GPL-3.0",
    "homepage": "http://alltubedownload.net/",
    "type": "project",
    "require": {
        "smarty/smarty": "~3.1",
        "rg3/youtube-dl": "2015.12.06",
        "slim/slim": "~2.6.2",
        "slim/views": "~0.1.3"
    },
    "require-dev": {
        "symfony/var-dumper": "~2.7.6"
    },
    "extra": {
        "paas": {
            "nginx-includes": [
                "nginx.conf"
            ]
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rg3/youtube-dl",
                "version": "2015.12.06",
                "source": {
                    "url": "https://github.com/rg3/youtube-dl.git",
                    "type": "git",
                    "reference": "2015.12.06"
                }
            }
        }
    ],
    "authors": [
        {
            "name": "Pierre Rudloff",
            "email": "contact@rudloff.pro",
            "homepage": "https://rudloff.pro/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alltube\\": "classes/",
            "Alltube\\Controller\\": "controllers/"
        }
    }
}
